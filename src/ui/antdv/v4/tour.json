{
  "name": "ATour",
  "props": {
    "arrow": {
      "value": "",
      "description": "是否显示箭头，包含是否指向元素中心的配置",
      "default": "`true`",
      "type": "`boolean` | `{ pointAtCenter: boolean}`"
    },
    "placement": {
      "value": "",
      "description": "引导卡片相对于目标元素的位置",
      "default": "`bottom`",
      "type": "`left` `leftTop` `leftBottom` `right` `rightTop` `rightBottom` `top` `topLeft` `topRight` `bottom` `bottomLeft` `bottomRight`"
    },
    "mask": {
      "value": "",
      "description": "是否启用蒙层，也可传入配置改变蒙层样式和填充色",
      "default": "`true`",
      "type": "`boolean` | `{ style?: CSSProperties; color?: string; }`"
    },
    "type": {
      "value": "",
      "description": "类型，影响底色与文字颜色",
      "default": "`default`",
      "type": "`default` | `primary`"
    },
    "open": {
      "value": "",
      "description": "打开引导",
      "default": "-",
      "type": "`boolean`"
    },
    "current(v-model)": {
      "value": "",
      "description": "当前处于哪一步",
      "default": "-",
      "type": "`number`"
    },
    "scrollIntoViewOptions": {
      "value": "",
      "description": "是否支持当前元素滚动到视窗内，也可传入配置指定滚动视窗的相关参数",
      "default": "`true`",
      "type": "`boolean` | `ScrollIntoViewOptions`"
    },
    "indicatorsRender": {
      "value": "",
      "description": "自定义指示器",
      "default": "-",
      "type": "`v-slot:indicatorsRender=\"{current, total}\"`"
    },
    "zIndex": {
      "value": "",
      "description": "Tour 的层级",
      "default": "`1001`",
      "type": "`number`"
    }
  },
  "methods": [],
  "events": [],
  "link": "https://antdv.com/components/tour-cn"
}
