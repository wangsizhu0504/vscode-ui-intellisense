{
  "name": "AInput",
  "props": {
    "addonAfter": {
      "value": "",
      "description": "带标签的 input，设置后置标签",
      "default": "",
      "type": "string|slot"
    },
    "addonBefore": {
      "value": "",
      "description": "带标签的 input，设置前置标签",
      "default": "",
      "type": "string|slot"
    },
    "allowClear": {
      "value": "",
      "description": "可以点击清除图标删除内容",
      "default": "",
      "type": "boolean"
    },
    "bordered": {
      "value": "",
      "description": "是否有边框",
      "default": "true",
      "type": "boolean"
    },
    "clearIcon": {
      "value": [
        "`<CloseCircleFilled",
        ">`"
      ],
      "description": "自定义清除图标 （allowClear 为 true 时生效）",
      "default": "`<CloseCircleFilled | >`",
      "type": "slot"
    },
    "defaultValue": {
      "value": "",
      "description": "输入框默认内容",
      "default": "",
      "type": "string"
    },
    "disabled": {
      "value": "",
      "description": "是否禁用状态，默认为 false",
      "default": "false",
      "type": "boolean"
    },
    "id": {
      "value": "",
      "description": "输入框的 id",
      "default": "",
      "type": "string"
    },
    "maxlength": {
      "value": "",
      "description": "最大长度",
      "default": "",
      "type": "number"
    },
    "prefix": {
      "value": "",
      "description": "带有前缀图标的 input",
      "default": "",
      "type": "string|slot"
    },
    "showCount": {
      "value": "",
      "description": "是否展示字数",
      "default": "false",
      "type": "boolean"
    },
    "status": {
      "value": "",
      "description": "设置校验状态",
      "default": "-",
      "type": "'error' | 'warning'"
    },
    "size": {
      "value": "",
      "description": "控件大小。注：标准表单内的输入框大小限制为 `middle`。可选 `large` `middle` `small`",
      "default": "-",
      "type": "string"
    },
    "suffix": {
      "value": "",
      "description": "带有后缀图标的 input",
      "default": "",
      "type": "string|slot"
    },
    "type": {
      "value": "",
      "description": "声明 input 类型，同原生 input 标签的 type 属性，见：[MDN](https://developer.mozilla.org/zh-CN/docs/Web/HTML/Element/input#属性)(请直接使用 `<a-textarea />` 代替 `type=\"textarea\"`)。",
      "default": "`text`",
      "type": "string"
    },
    "value(v-model)": {
      "value": "",
      "description": "输入框内容",
      "default": "",
      "type": "string"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "change",
      "description": "输入框内容变化时的回调",
      "params": "function(e)"
    },
    {
      "name": "pressEnter",
      "description": "按下回车的回调",
      "params": "function(e)"
    }
  ],
  "link": "https://antdv.com/components/input-cn"
}
