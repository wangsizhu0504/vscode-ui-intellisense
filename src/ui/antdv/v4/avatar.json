{
  "name": "AAvatar",
  "props": {
    "alt": {
      "value": "",
      "description": "图像无法显示时的替代文本",
      "default": "-",
      "type": "string"
    },
    "crossOrigin": {
      "value": "",
      "description": "cors 属性设置",
      "default": "-",
      "type": "`'anonymous'` | `'use-credentials'` | `''`"
    },
    "draggable": {
      "value": "",
      "description": "图片是否允许拖动",
      "default": "-",
      "type": "boolean | `'true'` | `'false'`"
    },
    "gap": {
      "value": "",
      "description": "字符类型距离左右两侧边界单位像素",
      "default": "4",
      "type": "number"
    },
    "icon": {
      "value": "",
      "description": "设置头像的图标类型，可设为 Icon 的 `type` 或 VNode",
      "default": "-",
      "type": "VNode | slot"
    },
    "loadError": {
      "value": "",
      "description": "图片加载失败的事件，返回 false 会关闭组件默认的 fallback 行为",
      "default": "-",
      "type": "() => boolean"
    },
    "shape": {
      "value": "",
      "description": "指定头像的形状",
      "default": "circle",
      "type": "circle | square"
    },
    "size": {
      "value": "",
      "description": "设置头像的大小",
      "default": "",
      "type": "number | `large` | small |  | { xs: number, sm: number, ...}"
    },
    "src": {
      "value": "",
      "description": "图片类头像的资源地址",
      "default": "-",
      "type": "string"
    },
    "srcset": {
      "value": "",
      "description": "设置图片类头像响应式资源地址",
      "default": "-",
      "type": "string"
    }
  },
  "methods": [],
  "events": [],
  "link": "https://antdv.com/components/avatar-cn"
}
