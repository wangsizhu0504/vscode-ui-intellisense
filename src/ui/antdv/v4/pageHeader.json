{
  "name": "APageHeader",
  "props": {
    "avatar": {
      "value": "",
      "description": "标题栏旁的头像",
      "default": "-",
      "type": "[avatar props]( | components | avatar-cn | )"
    },
    "backIcon": {
      "value": [
        "`<ArrowLeft",
        ">`"
      ],
      "description": "自定义 back icon ，如果为 false 不渲染 back icon",
      "default": "`<ArrowLeft | >`",
      "type": "string|slot"
    },
    "breadcrumb": {
      "value": "",
      "description": "面包屑的配置",
      "default": "-",
      "type": "[breadcrumb]( | components | breadcrumb-cn | )"
    },
    "extra": {
      "value": "",
      "description": "操作区，位于 title 行的行尾",
      "default": "-",
      "type": "string|slot"
    },
    "footer": {
      "value": "",
      "description": "PageHeader 的页脚，一般用于渲染 TabBar",
      "default": "-",
      "type": "string|slot"
    },
    "ghost": {
      "value": "",
      "description": "pageHeader 的类型，将会改变背景颜色",
      "default": "true",
      "type": "boolean"
    },
    "subTitle": {
      "value": "",
      "description": "自定义的二级标题文字",
      "default": "-",
      "type": "string|slot"
    },
    "tags": {
      "value": "",
      "description": "title 旁的 tag 列表",
      "default": "-",
      "type": "[Tag]( | components | tag-cn | )\\[] | [Tag]( | components | tag-cn | )"
    },
    "title": {
      "value": "",
      "description": "自定义标题文字",
      "default": "-",
      "type": "string|slot"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "back",
      "description": "返回按钮的点击事件",
      "params": "function(e)"
    }
  ],
  "link": "https://antdv.com/components/page-header"
}
