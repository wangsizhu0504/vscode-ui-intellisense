{
  "name": "APagination",
  "props": {
    "current(v-model)": {
      "value": "",
      "description": "当前页数",
      "default": "-",
      "type": "number"
    },
    "defaultPageSize": {
      "value": "",
      "description": "默认的每页条数",
      "default": "10",
      "type": "number"
    },
    "disabled": {
      "value": "",
      "description": "禁用分页",
      "default": "false",
      "type": "boolean"
    },
    "hideOnSinglePage": {
      "value": "",
      "description": "只有一页时是否隐藏分页器",
      "default": "false",
      "type": "boolean"
    },
    "itemRender": {
      "value": "",
      "description": "用于自定义页码的结构，可用于优化 SEO",
      "default": "-",
      "type": "({page, type: 'page' | 'prev' | 'next', originalElement}) => vNode | v-slot"
    },
    "pageSize(v-model)": {
      "value": "",
      "description": "每页条数",
      "default": "-",
      "type": "number"
    },
    "pageSizeOptions": {
      "value": "",
      "description": "指定每页可以显示多少条",
      "default": "\\['10', '20', '30', '40']",
      "type": "string\\[]"
    },
    "responsive": {
      "value": "",
      "description": "当 size 未指定时，根据屏幕宽度自动调整尺寸",
      "default": "false",
      "type": "boolean"
    },
    "showLessItems": {
      "value": "",
      "description": "是否显示较少页面内容",
      "default": "false",
      "type": "boolean"
    },
    "showQuickJumper": {
      "value": "",
      "description": "是否可以快速跳转至某页",
      "default": "false",
      "type": "boolean"
    },
    "showSizeChanger": {
      "value": "",
      "description": "是否展示 `pageSize` 切换器，当 `total` 大于 50 时默认为 true",
      "default": "false",
      "type": "boolean"
    },
    "showTotal": {
      "value": "",
      "description": "用于显示数据总量和当前数据顺序",
      "default": "-",
      "type": "Function(total, range)"
    },
    "simple": {
      "value": "",
      "description": "当添加该属性时，显示为简单分页",
      "default": "false",
      "type": "boolean"
    },
    "size": {
      "value": "",
      "description": "当为「small」时，是小尺寸分页",
      "default": "\"\"",
      "type": "string"
    },
    "total": {
      "value": "",
      "description": "数据总数",
      "default": "0",
      "type": "number"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "change",
      "description": "页码或 `pageSize` 改变的回调，参数是改变后的页码及每页条数",
      "params": "Function(page, pageSize)"
    },
    {
      "name": "showSizeChange",
      "description": "pageSize 变化的回调",
      "params": "Function(current, size)"
    }
  ],
  "link": "https://antdv.com/components/pagination"
}
