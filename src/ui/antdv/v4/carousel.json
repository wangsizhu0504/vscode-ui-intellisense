{
  "name": "ACarousel",
  "props": {
    "autoplay": {
      "value": "",
      "description": "是否自动切换",
      "default": "false",
      "type": "boolean"
    },
    "dotPosition": {
      "value": "",
      "description": "面板指示点位置，可选 `top` `bottom` `left` `right`",
      "default": "bottom",
      "type": "string"
    },
    "dots": {
      "value": "",
      "description": "是否显示面板指示点",
      "default": "true",
      "type": "boolean"
    },
    "dotsClass": {
      "value": "",
      "description": "面板指示点类名",
      "default": "`slick-dots`",
      "type": "string"
    },
    "easing": {
      "value": "",
      "description": "动画效果",
      "default": "`linear`",
      "type": "string"
    },
    "effect": {
      "value": "",
      "description": "动画效果函数",
      "default": "`scrollx`",
      "type": "`scrollx` | `fade`"
    },
    "afterChange": {
      "value": "",
      "description": "切换面板的回调",
      "default": "-",
      "type": "function(current)"
    },
    "beforeChange": {
      "value": "",
      "description": "切换面板的回调",
      "default": "-",
      "type": "function(from, to)"
    },
    "goTo(slideNumber, dontAnimate)": {
      "value": "",
      "description": "切换到指定面板, dontAnimate = true 时，不使用动画",
      "default": "",
      "type": "default"
    },
    "next()": {
      "value": "",
      "description": "切换到下一面板",
      "default": "",
      "type": "default"
    },
    "prev()": {
      "value": "",
      "description": "切换到上一面板",
      "default": "",
      "type": "default"
    }
  },
  "methods": [],
  "events": [],
  "link": "https://antdv.com/components/carousel-cn"
}
