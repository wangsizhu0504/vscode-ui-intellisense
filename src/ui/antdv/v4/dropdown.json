{
  "name": "ADropdown",
  "props": {
    "arrow": {
      "value": "",
      "description": "下拉框箭头是否显示",
      "default": "false",
      "type": "boolean | { pointAtCenter: boolean }"
    },
    "destroyPopupOnHide": {
      "value": "",
      "description": "关闭后是否销毁 Dropdown",
      "default": "false",
      "type": "boolean"
    },
    "disabled": {
      "value": "",
      "description": "菜单是否禁用",
      "default": "false",
      "type": "boolean"
    },
    "getPopupContainer": {
      "value": "",
      "description": "菜单渲染父节点。默认渲染到 body 上，如果你遇到菜单滚动定位问题，试试修改为滚动的区域，并相对其定位。",
      "default": "`() => document.body`",
      "type": "Function(triggerNode)"
    },
    "overlay(v-slot)": {
      "value": "",
      "description": "菜单",
      "default": "-",
      "type": "[Menu]( | components | menu-cn)"
    },
    "overlayClassName": {
      "value": "",
      "description": "下拉根元素的类名称",
      "default": "-",
      "type": "string"
    },
    "overlayStyle": {
      "value": "",
      "description": "下拉根元素的样式",
      "default": "-",
      "type": "object"
    },
    "placement": {
      "value": "",
      "description": "菜单弹出位置",
      "default": "bottomLeft",
      "type": "bottomLeft | bottom | `bottomRight` | `topLeft` | top | `topRight`"
    },
    "trigger": {
      "value": "",
      "description": "触发下拉的行为, 移动端不支持 hover",
      "default": "['hover']",
      "type": "['click' | 'hover' | 'contextmenu']"
    },
    "open(v-model)": {
      "value": "",
      "description": "菜单是否显示",
      "default": "false",
      "type": "boolean"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "openChange",
      "description": "菜单显示状态改变时调用，参数为 visible。点击菜单按钮导致的消失不会触发",
      "params": "function(open)"
    }
  ],
  "link": "https://antdv.com/components/dropdown-cn"
}
