{
  "name": "ARate",
  "props": {
    "allowClear": {
      "value": "",
      "description": "是否允许再次点击后清除",
      "default": "true",
      "type": "boolean"
    },
    "allowHalf": {
      "value": "",
      "description": "是否允许半选",
      "default": "false",
      "type": "boolean"
    },
    "autofocus": {
      "value": "",
      "description": "自动获取焦点",
      "default": "false",
      "type": "boolean"
    },
    "character": {
      "value": [
        "`<StarOutlined",
        ">`"
      ],
      "description": "自定义字符",
      "default": "`<StarOutlined | >`",
      "type": "string | slot"
    },
    "count": {
      "value": "",
      "description": "star 总数",
      "default": "5",
      "type": "number"
    },
    "disabled": {
      "value": "",
      "description": "只读，无法进行交互",
      "default": "false",
      "type": "boolean"
    },
    "tooltips": {
      "value": "",
      "description": "自定义每项的提示信息",
      "default": "-",
      "type": "string[]"
    },
    "value(v-model)": {
      "value": "",
      "description": "当前数，受控值",
      "default": "-",
      "type": "number"
    }
  },
  "methods": [],
  "events": [
    {
      "name": "blur",
      "description": "失去焦点时的回调",
      "params": "Function()"
    },
    {
      "name": "change",
      "description": "选择时的回调",
      "params": "Function(value: number)"
    },
    {
      "name": "focus",
      "description": "获取焦点时的回调",
      "params": "Function()"
    },
    {
      "name": "hoverChange",
      "description": "鼠标经过时数值变化的回调",
      "params": "Function(value: number)"
    },
    {
      "name": "keydown",
      "description": "按键回调",
      "params": "Function(event)"
    }
  ],
  "link": "https://antdv.com/components/rate-cn"
}
